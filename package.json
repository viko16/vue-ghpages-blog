{
  "name": "vue-ghpages-blog",
  "description": "A blog based on GitHub Pages built with Vue.js 2 + webpack 2",
  "version": "1.2.0",
  "main": "src/main.js",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --open",
    "prebuild": "rimraf dist/*",
    "build": "cross-env NODE_ENV=production webpack -p --progress --hide-modules",
    "lint": "eslint --ext \".js, .vue\" src test --fix",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=testing karma start test/unit/karma.conf.js"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viko16/vue-ghpages-blog.git"
  },
  "keywords": [
    "blog",
    "github",
    "vuejs",
    "webpack"
  ],
  "author": "viko16 (https://github.com/viko16)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/viko16/vue-ghpages-blog/issues"
  },
  "homepage": "https://github.com/viko16/vue-ghpages-blog#readme",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.3.3",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-chai-friendly": "^0.3.6",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^3.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.0",
    "html-webpack-plugin": "^2.22.0",
    "inject-loader": "^3.0.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon-chai": "^1.2.4",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.0",
    "mocha": "^3.1.2",
    "rimraf": "^2.5.4",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.17.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.0",
    "vue-hot-reload-api": "^2.0.0",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^12.0.0",
    "vue-style-loader": "^3.0.0",
    "vue-template-compiler": "^2.1.0",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  },
  "dependencies": {
    "axios": "^0.16.0",
    "es6-promise": "^4.0.0",
    "front-matter": "^2.1.0",
    "marked": "^0.3.6",
    "normalize.css": "^7.0.0",
    "prismjs": "^1.5.1",
    "timeago.js": "^3.0.0",
    "vue": "^2.2.6",
    "vue-router": "^2.0.0"
  }
